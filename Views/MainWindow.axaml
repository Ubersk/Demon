<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Kruchinin_28092022.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Kruchinin_28092022.Views.MainWindow"
        Icon="/Assets/Vosmerka.ico"
        Title="Восьмерка"
		>
	<Window.Background>
		<ImageBrush Source="/Assets/Vosmerka.png"
					Opacity="0.4"/>
	</Window.Background>
    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>


	<Grid
		  ShowGridLines="True">
		
		<Grid.RowDefinitions>
			<RowDefinition Height="50"/>
			<RowDefinition/>
			<RowDefinition Height="60"/>
		</Grid.RowDefinitions>
		
		<StackPanel
			Grid.Row="0"
			Height="40"
			Orientation="Horizontal">
			<Image
			Margin="15,0,0,0"
			Source="/Assets/Vosmerka.png"></Image>
			<TextBox
				Margin="27,2,20,80"
				Height="20"
				Width="300"
				Watermark="Введите для поиска">
				
			</TextBox>
			<ComboBox
				
				Margin="27,2,20,1"
				Width="150"
				></ComboBox>
			<ComboBox
				Margin="27,2,20,1"
				Width="150"
				></ComboBox>
		</StackPanel>
		<ListBox
			Opacity="0.6"
			Grid.Row="1">
		</ListBox>
	</Grid>

</Window>
